<div class="col-xl-3 col-lg-3 col-md-3 col-sm-3 col logo_section">
  <div class="full">
    <div class="center-desk">
      <div class="logo">
        {{#if image}}
        <a href="{{href}}"><img src="{{image}}" width="{{width}}" height="{{height}}" alt="#"></a>
        {{else}}
        <a href="/"><img src="/images/logo.png" alt="#" width="100" height="100"></a>
        {{/if}}
      </div>
    </div>
  </div>
</div>
